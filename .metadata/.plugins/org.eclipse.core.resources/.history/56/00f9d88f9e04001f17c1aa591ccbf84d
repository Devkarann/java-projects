package question_6;

import java.util.Map;

public class findMyAccount {

	public static void findAadhar(String aadhar, Map<Integer, Employee> empList)throws ErrorMain {
		
		Employee e = empList.get(aadhar);
		if(e != null) {
			return throw new ErrorMain("Duplicate entry");
			
		}
		return e;
		
	}

}
