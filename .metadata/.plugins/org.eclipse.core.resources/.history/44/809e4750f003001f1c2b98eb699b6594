package question_5;

public class Order {
	private static int orderId;
	private static int oId;
	static {
		oId = 1;
	}
	
	private int petId;
	private int quantity;
	private Status statusType;
	
	public Order(int petId, int quantity, Status statusType) {
		super();
		this.orderId = oId;
		this.petId = petId;
		this.quantity = quantity;
		this.statusType = statusType;
	}
	
	
	
	
	
	
}
